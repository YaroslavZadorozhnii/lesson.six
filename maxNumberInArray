/*
1) Напишите метод который вернет 
максимальное число из массива целых чисел.
*/

public class Main {
	 static int maxNumber (int[] a ) {
			int maxNumber = a[0];
			for(int i = 1; i < a.length; i++) {
				maxNumber = Math.max(maxNumber, a[i]);
		}
			return maxNumber;
		}
	public static void main(String[] args) {
		int[] a = new int[] {12,24,1,3,6,587,1,4,1,4,6,1,24,
				64,13,46,61,4,13,5,323,48,86,26};
		
		System.out.println(maxNumber(a));		
	}

}
